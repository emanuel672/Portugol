programa
{
	funcao inicio()
	{
		real a, b, soma, sub, mult, div
		
		escreva("digite o primeiro numero: ")
		leia(a)
		
		escreva ("digite o segundo numero: ")
		leia(b)
		
		soma = a + b
		sub = a - b
		mult = a * b
		div = a / b
		
		escreva (" \nA soma dos numeros e igual:", soma)
		escreva (" \nA subtracao dos numeros e igual:", sub)
		escreva (" \nA multiplicacao dos numeros e igual:", mult)
		escreva(" \nA divisao dos numeros e igual:", div)
	}

}
